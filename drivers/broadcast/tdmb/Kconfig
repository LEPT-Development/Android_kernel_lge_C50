#menu "BROADCAST_DEVICE"

config LGE_BROADCAST_TDMB_FC8080
    bool "LGE broacast TDMB FC8080 device"
    depends on (LGE_BROADCAST_TDMB && MACH_MSM8916_YG_SKT_KR)
    default y
    help
      Select this if you want to use broadcast service

